{
  "expo": {
    "name": "ecpower",
    "slug": "ecpower",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "ecpower",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/logo1.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.ecpower",
      "icon": "./assets/images/logo.png"
    },
    "android": {
      "icon": "./assets/images/logo.png",
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/logo.png",
        "backgroundImage": "./assets/images/logo.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.ecpower",
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "ACCESS_WIFI_STATE"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/logo.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo1.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "kotlinVersion": "2.0.21",
            "usesCleartextTraffic": true,
            "networkSecurityConfig": "allow-all"
          }
        }
      ],
      "expo-secure-store",
      "expo-router"
    ],
    "experiments": {},
    "extra": {
      "eas": {
        "projectId": "281b0ff4-31b1-4d0c-8b10-9377230ffe61"
      }
    },
    "owner": "sumiran.b"
  },
  "build": {
    "apk": {
      "android": {
        "buildType": "apk"
      }
    }
  }
}